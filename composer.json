{
  "name": "next-smallest-number-finder/next-smallest-number-finder",
  "description": "A PHP application to find the next smallest number in a dataset.",
  "type": "project",
  "require": {
    "php": ">=8.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6"
  },
  "scripts": {
    "test": "phpunit --colors=always --testdox tests"
  }
}
